{
  "grammars": [
    {
      "name": "tryouts",
      "camelcase": "Tryouts",
      "scope": "source.tryouts",
      "rules": {
        "source": {
          "type": "STRING",
          "named": true
        }
      },
      "file-types": [
        "rb",
        "tryouts"
      ],
      "content-regex": "\\.(rb|tryouts)$",
      "injection-regex": "tryouts"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Tryouts grammar for tree-sitter",
    "authors": [
      {
        "name": "DM"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-tryouts"
    }
  },
  "bindings": {
    "c": false,
    "go": false,
    "node": false,
    "python": false,
    "rust": false,
    "swift": false
  }
}
