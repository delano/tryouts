{
  "name": "tryouts",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "setup_section"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "tryout_block"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "teardown_section"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "setup_section": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "comment"
          }
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "require_statement"
          }
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "assignment_statement"
              },
              {
                "type": "SYMBOL",
                "name": "configuration_statement"
              },
              {
                "type": "SYMBOL",
                "name": "any_statement"
              }
            ]
          }
        }
      ]
    },
    "teardown_section": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "any_statement"
      }
    },
    "tryout_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "tryout_description"
          }
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "code_line"
          }
        },
        {
          "type": "SYMBOL",
          "name": "expectation"
        }
      ]
    },
    "tryout_description": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "##"
        },
        {
          "type": "PATTERN",
          "value": "[^\\n]+"
        }
      ]
    },
    "expectation": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#=>"
        },
        {
          "type": "PATTERN",
          "value": "[^\\n]+"
        }
      ]
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "PATTERN",
          "value": "[^\\n]*"
        }
      ]
    },
    "require_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "require"
        },
        {
          "type": "SYMBOL",
          "name": "string_literal"
        }
      ]
    },
    "assignment_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@"
        },
        {
          "type": "PATTERN",
          "value": "[a-zA-Z_][a-zA-Z0-9_]*"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "any_value"
        }
      ]
    },
    "configuration_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "[A-Z][a-zA-Z0-9:]*\\.(configure|boot|path)"
        },
        {
          "type": "PATTERN",
          "value": "[^\\n]+"
        }
      ]
    },
    "code_line": {
      "type": "PATTERN",
      "value": "[^#][^\\n]*"
    },
    "string_literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "'"
            },
            {
              "type": "PATTERN",
              "value": "[^']*"
            },
            {
              "type": "STRING",
              "value": "'"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "\""
            },
            {
              "type": "PATTERN",
              "value": "[^\"]*"
            },
            {
              "type": "STRING",
              "value": "\""
            }
          ]
        }
      ]
    },
    "any_value": {
      "type": "PATTERN",
      "value": "[^\\n]+"
    },
    "any_statement": {
      "type": "PATTERN",
      "value": "[^\\n]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
