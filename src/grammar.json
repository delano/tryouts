{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "tryouts",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "setup_section"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "testcase"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "teardown_section"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "setup_section": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "non_description_line"
      }
    },
    "teardown_section": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "non_description_line"
      }
    },
    "testcase": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "description_line"
          }
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "code_line"
          }
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "expectation_line"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "blank_line"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "description_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "##"
        },
        {
          "type": "PATTERN",
          "value": "[^\\n]*"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "code_line": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "#"
            },
            {
              "type": "PATTERN",
              "value": "[^#=>\\n][^\\n]*"
            },
            {
              "type": "STRING",
              "value": "\n"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "PATTERN",
              "value": "[^#\\n][^\\n]*"
            },
            {
              "type": "STRING",
              "value": "\n"
            }
          ]
        }
      ]
    },
    "expectation_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#=>"
        },
        {
          "type": "PATTERN",
          "value": "[^\\n]*"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "blank_line": {
      "type": "PATTERN",
      "value": "\\s*\\n"
    },
    "non_description_line": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "PATTERN",
              "value": "[^#\\n][^\\n]*"
            },
            {
              "type": "STRING",
              "value": "\n"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "#"
            },
            {
              "type": "PATTERN",
              "value": "[^#\\n][^\\n]*"
            },
            {
              "type": "STRING",
              "value": "\n"
            }
          ]
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
